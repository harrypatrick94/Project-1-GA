<div class="user-image-container">
  <div class="user-image">
    <%= link_to cl_image_tag(@current_user.image), user_path(@current_user.id) %>
  </div>
</div>
<div class="song-greeting">
  <div class="sing-info">
    <h1><%= @song.name %></h1>

    <h2><%= @song.artist %></h2>
  </div>
</div>
      <div id="SongsController">
        <div class="piano-container">
          <div class="piano">
            <ul id="piano">
              <li class="key" >
                <div class="black-key"><span id="s">S</span></div>
                <div class="white-key">
                  <span id="z">Z</span>
                </div>
              </li>
              <li class="key" >
                <div class="black-key"><span id="d">D</span></div>
                <span id="x">X</span>
              </li>
              <li  class="key" >
                <span id="c">C</span>
              </li>
              <li class="key" >
                <div class="black-key"><span id="g">G</span></div>
                <span id="v">V</span>
              </li>
              <li class="key" >
                <div  class="black-key"><span id="h">H</span></div>
                <span id="b">B</span>
              </li>
              <li class="key" >
                <div class="black-key"><span id="j">J</span></div>
                <span id="n">N</span>
              </li>
              <li class="key" >
                <span id="m">M</span>
              </li>
            </ul>
          </div>
        </div>
      </div>

<p>

</p>

<div class="song-nav">
  <ul class="song-nav-ul">
    <li class="song-nav-li"><nav><strong><%= link_to "Edit #{@song.name}", edit_song_path(@song.id) %></strong></nav></li>
    <li class="song-nav-li"><nav><strong><%= link_to "Back to Genre Index", user_path(session[:user_id]) %></strong></nav>
    </li>
    <li>
      <nav><strong><%= link_to "Back to #{@genre_name}", genre_path(@genre_id) %></strong></nav>
    </li>
    <li class="song-nav-li"><strong><nav><%= link_to 'Delete Song', song_path( @song.id), method: 'delete'%></nav></strong></li>
    <li class="user-nav-li"><nav><strong><%= link_to "Concerts near me", concerts_map_path %></strong></nav></li>

  </ul>
</div>


  <%= javascript_include_tag 'zpiano' %>
