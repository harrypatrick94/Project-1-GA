<div class="user-image-container">
  <div class="user-image">
    <%= link_to cl_image_tag(@current_user.image), user_path(@current_user.id) %>
  </div>
</div>
<div class="genre-edit-container">
  <div>
    <h1>Edit <%= @genre.name %></h1>
  </div>
</div>
<%= render partial: 'form' %>
<footer class="genre-nav">
  <ul class="genre-nav-ul">
    <li class="genre-nav-li">
      <nav><strong><%= link_to "Genres Index", user_path(@current_user.id) %></strong></nav>
    </li>
    <li class="genre-nav-li">
      <nav><strong><%= link_to "Back to #{@genre.name}", genre_path( @genre.id) %></strong></nav>
    </li>
  </ul>
</footer>
